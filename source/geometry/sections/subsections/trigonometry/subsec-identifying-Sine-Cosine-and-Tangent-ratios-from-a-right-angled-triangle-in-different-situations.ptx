                            \node [black]at (4.5,1.5) {\textbf{3 cm}}; % Opposite side
                            <!-- \node at (2,2) {10 cm}; % Hypotenuse -->

                            % Label the right angle
                            \draw [very thick] (4,0) -- (3.7,0) -- (3.7,0.3) -- (4,0.3) -- cycle;
                            

                            % Mark the angle theta at the hypotenuse-adjacent junction
                            \draw (0.5,0) arc[start angle=0,end angle=36.87,radius=0.5cm];
                            \node at (0.8,0.3) {\(\beta\)}; % Place theta at correct position
                        \end{tikzpicture}
                    </latex-image>
                    
                </image>
            </statement>

            <solution>
            <p>
                   <md>
                        <mrow>tan \,\beta= \amp \frac{Opposite}{Adjacent}</mrow>
                        <mrow> =\amp \frac{3}{4} </mrow>
                        <mrow> =\amp  0.75</mrow>
                 </md>
            </p>
            </solution>
        </example>
        
